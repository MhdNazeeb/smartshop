<script src="https://unpkg.com/sweetalert/dist/sweetalert.min.js"></script>
<script src="https://cdn.jsdelivr.net/npm/axios/dist/axios.min.js"></script>
<script>
    const EditBanner=(id)=>{
        console.log('thiscorrect');
        event.preventDefault()
        let bannerData=document.getElementById('banneredit')
        console.log(bannerData,'this is banner data');
        const formData = new FormData(bannerData);
        console.log(formData,'this fordata');
        position=[]
        for(i=0;i<1;i++){
            if(document.getElementById(i).value)
            position.push(i)
        }
        formData.append("position", JSON.stringify(position))
        
        axios.put(`banner?bannerid=${id}`,formData,{
     headers:{"Content-Type": "multipart/form-data"}
    
    }).then((res)=>{
        if(res.data.updated){
            swal("Good job!", "update", "success");
        }
    })
    }
    
   
</script>
